
import java.util.*;
class Sort
{
public static void main(String args[])
{
	Scanner s=new Scanner(System.in);
	int n=s.nextInt();
	int a[]=new int[n];
	for(int i=0;i<n;i++)
	{
		a[i]=s.nextInt();
	}
	int mid;
	if(n%2==0)
		mid=n/2
	else
		mid=(n/2)+1;
	for(int i=0;i<mid-1;i++)
	{
		for(j=i+1;j<mid;j++)
		{
			if(a[i]>a[j])
			{
			int t=a[j];
			a[j]=a[j+1];
			a[j+1]=t;
			}
		}
}
	for(int i=0;i<n;i++)
	{
		System.out.println(a[i]+" ");
	}
}
}
